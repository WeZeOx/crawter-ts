// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referencialActions"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model TrendLive {
 Id Int @id
 SpecTop String
 NameTop String
 Trend Trend @relation(fields: [TrendId], references: [Id], onUpdate: Cascade, onDelete: Cascade)
 TrendId Int
}

model Trend {
 Id Int @id
 Views String
 Follow String
 Category String
 Tag String
 TrendLive TrendLive[]
}